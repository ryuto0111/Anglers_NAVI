<div class="container">
  <div class="row">
    <div class="col-md-4 mx-auto">
      <%= render 'layouts/error_messages', obj: @tag %>
      <h4>タグ編集</h4>
        <%= form_with model: @tag do |f| %>
          <div class="form">
            <p><b>タグ</b></p>
            <%= f.text_field :name %>
            <%= f.submit '変更を保存', class:"btn btn-success" %>
          </div>
        <% end %>
    </div>
  </div>
</div>